{
  "name": "backend",
  "version": "1.0.0",
  "description": "Backend API for rental contract management",
  "main": "server.js",
  "type": "commonjs",
  "scripts": {
    "dev": "ts-node server.ts",
    "build:emails": "tsc -p tsconfig.emails.json",
    "build": "npm run build:emails && tsc",
    "start": "node dist/server.js",
    "migrate:make": "knex migrate:make --knexfile db/knexfile.ts -x ts",
    "migrate:latest": "knex migrate:latest --knexfile db/knexfile.ts",
    "migrate:rollback": "knex migrate:rollback --knexfile db/knexfile.ts",
    "migrate:status": "knex migrate:status --knexfile db/knexfile.ts",
    "seed:make": "knex seed:make --knexfile db/knexfile.ts -x ts",
    "seed:run": "knex seed:run --knexfile db/knexfile.ts",
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "keywords": [
    "rental",
    "contracts",
    "api"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@react-email/components": "^0.5.6",
    "@react-email/render": "^1.3.2",
    "@types/express": "^5.0.3",
    "@types/node": "^24.7.0",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dayjs": "^1.11.18",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "node-cron": "^4.2.1",
    "pg": "^8.16.3",
    "react": "^19.2.0",
    "resend": "^6.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "zod": "^4.1.12",
    "@types/react": "^19.2.2"
  },
  "devDependencies": {
    "@react-email/preview-server": "^4.3.0",
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/express-rate-limit": "^5.1.3",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node-cron": "^3.0.11",

    "@types/supertest": "^6.0.3",
    "cross-env": "^10.1.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "react-email": "^4.3.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.1.1"
  }
}
