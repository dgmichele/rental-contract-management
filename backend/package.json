{
  "name": "backend",
  "version": "1.0.0",
  "description": "Backend API for rental contract management",
  "main": "server.js",
  "type": "commonjs",
  "scripts": {
    "dev": "ts-node server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "migrate:make": "knex migrate:make --knexfile db/knexfile.ts -x ts",
    "migrate:latest": "knex migrate:latest --knexfile db/knexfile.ts",
    "migrate:rollback": "knex migrate:rollback --knexfile db/knexfile.ts",
    "migrate:status": "knex migrate:status --knexfile db/knexfile.ts",
    "seed:make": "knex seed:make --knexfile db/knexfile.ts -x ts",
    "seed:run": "knex seed:run --knexfile db/knexfile.ts",
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "keywords": ["rental", "contracts", "api"],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^5.0.3",
    "@types/node": "^24.7.0",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "knex": "^3.1.0",
    "pg": "^8.16.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1"
  }
}